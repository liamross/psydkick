{
  "name": "psydkick-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "component": "node scripts/component/component.js -p='src/components' -s='psy-'",
    "__eject": "react-scripts eject",
    "precodegen": "rm -rf src/**/types && rm -rf types",
    "codegen": "apollo client:codegen types --target=typescript"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@blueprintjs/core": "^3.12.0",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "alpha",
    "apollo-link-http": "^1.5.9",
    "classnames": "^2.2.6",
    "graphql-tag": "^2.10.1",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.0-alpha.1",
    "react-apollo": "^2.4.0",
    "react-dom": "^16.8.0-alpha.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "apollo": "^2.3.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react-scripts": "^2.1.3",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
