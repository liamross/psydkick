{
  "name": "psydkick-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "nodemon build/index.js",
    "watch": "rm -rf build && tsc --watch",
    "build": "rm -rf build && tsc",
    "precodegen": "apollo service:download --endpoint=http://localhost:4000/graphql",
    "codegen": "apollo codegen:generate src/types/api.d.ts --target=typescript --outputFlat",
    "new": "apollo schema:download --endpoint=https://api.github.com/graphql --header='Authorization: bearer ***' apollo client:codegen --target=typescript"
  },
  "dependencies": {
    "apollo-server": "^2.3.1",
    "graphql": "^14.0.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.6",
    "tslib": "^1.9.3",
    "typeorm": "^0.2.11"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "apollo": "^2.1.9",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
